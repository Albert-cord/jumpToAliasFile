{
  "name": "jump-to-alias-file",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "jumpToAliasFile setting",
      "properties": {
        "jumpToAliasFile.alias": {
          "type": "object",
          "default": {},
          "scope": "resource",
          "description": "同 webpack resolve.alias 配置"
        },
        "jumpToAliasFile.webpeckConfigPath": {
          "type": "string",
          "default": "",
          "scope": "resource",
          "description": "webpack config 路径"
        }
      }
    }
  },
  "devDependencies": {
    "@types/node": "^6.0.40",
    "tslint": "^5.11.0",
    "typescript": "^2.1.4",
    "vscode": "^1.1.17"
  }
}
